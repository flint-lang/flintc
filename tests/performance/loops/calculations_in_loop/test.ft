// Calculate the square root of a number using Babylonians method
def square_root(int n) -> int:
	// Since sqrt(0) = 0 and sqrt(1) = 1
	if n == 0:
		return n;
	else if n == 1:
		return n;

	int x = n;
	int y = (x + 1) / 2;

	while y < x:
		x = y;
		y = (x + n / x) / 2;

	return x;

// Calculate the sqrt of every number from 0 to 100000
def main():
	int num = 0;
	int sum = 0;
	while num < 10000000:
		int sqrt = square_root(num);
		num = num + 1;
		sum = sum + sqrt;
	print(sum);
