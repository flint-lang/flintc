use Core.print

data DTransform:
    i32x2 pos;
    i32x2 size;
    DTransform(pos, size);

data DScale:
    f32x2 scale;
    DScale(scale);

func FMove requires(DTransform trans, DScale scale):
    def print():
        print($"pos: {trans.pos}, size: {trans.size}, scale: {scale.scale}\n");

    def get_pos() -> i32x2:
        return trans.pos;

entity Entity:
    data: DTransform;
    func: FMove;
    Entity(DTransform);

def main():
    e := Entity(DTransform(i32x2(100, 120), i32x2(200, 220)));
    e.print();
    print($"e.pos = {e.get_pos()}\n");
