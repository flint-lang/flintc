use Core.assert

def add(i32 x, i32 y) -> i32:
    return x + y;

def add_str(str a, str b) -> str:
    return a + b;

def test_str():
    str result;
    for i32 i = 0; i < 5; i++:
        for i32 j = 0; j < 5; j++:
            i32 res = add(i, j);
            result = result + str(res);
    assert(result == "0123412345234563456745678");
    assert("a" < "b");
    assert("a" != "b");
    assert("a" < "ab");
    assert("A" < "z");
    assert("az" > "aZ");
    assert("ab" > "a");
    assert("1" <= "2");
    assert("4" >= "3");
    assert("a" != "b");
    assert(add_str("A", "B") == "AB");
    assert(str(add(4, 5)) == "9");
