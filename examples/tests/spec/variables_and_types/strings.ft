use Core.assert

test "0. Add strings":
	str s1 = "Hello, ";
	str s2 = "There!";
	str s3 = s1 + s2;
	assert(s3 == "Hello, There!");
	assert(s3.len == 13);

test "1. Append strings":
	str result;
	for i32 i = 0; i < 5; i++:
		for i32 j = 0; j < 5; j++:
			result += i + j;
	assert(result == "0123412345234563456745678");
	assert(result.length == 25);

test "2. Cmp lt same size":
	assert("a" < "b");

test "3. Cmp neq same size":
    assert("a" != "b");

test "4. Cmp lt":
    assert("a" < "ab");

test "5. Cmp lt Upcase":
    assert("A" < "z");

test "6. Cmp gt Upcase":
    assert("az" > "aZ");

test "7. Cmp gt bigger":
    assert("ab" > "a");

test "8. Cmp le same size":
    assert("1" <= "2");

test "9. Cmp ge same size":
    assert("4" >= "3");

test "10. String interpolate bool 'true'":
	bool b = true;
	str s = $"b = {b}";
	assert(s == "b = true");

test "11. String interpolate bool 'false'":
	bool b = false;
	str s = $"b = {b}";
	assert(s == "b = false");

test "12. String interpolate bool8":
	bool8 b8 = u8(27);
	str s = $"b8 = {b8}";
	assert(s == "b8 = 00011011");

test "13. String interpolate u8":
	u8 c = 'A';
	str s = $"c = {c}";
	assert(s == "c = A");

test "14. String interpolate u32":
	u32 u = 1_230;
	str s = $"u = {u}";
	assert(s == "u = 1230");

test "15. String interpolate u64":
	u64 u = 321_230;
	str s = $"u = {u}";
	assert(s == "u = 321230");

test "16. String interpolate i32":
