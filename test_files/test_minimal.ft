use Core.print
use Core.time

def slow_calculation() -> i32:
    i32 sum = 0;
    for i := 0; i < 100; i++:
		sleep(10, TimeUnit.MS);
        sum += 1;
    return sum;

def main():
    TimeStamp start = now();
    i32 result = slow_calculation();
    TimeStamp end = now();

    Duration elapsed = duration(start, end);
    print($"Calculation took {as_unit(elapsed, TimeUnit.S)} s\n");
