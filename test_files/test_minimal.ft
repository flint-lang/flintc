use Core.print

data Vec2:
	i32 x;
	i32 y;
	Vec2(x, y);

data Nested:
	Vec2 v1;
	Vec2 v2;
	Nested(v1, v2);

def create_vec(i32 x, i32 y) -> Vec2:
	return Vec2(x, y);

def main():
	Vec2? vec = create_vec(10, 20);
	print($"(x, y) = ({vec!.x}, {vec!.y})\n");

	Nested n = Nested(Vec2(10, 20), Vec2(30, 40));
	print($"n.v1.(x, y) = ({n.v1.x}, {n.v1.y})\n");
	print($"n.v2.(x, y) = ({n.v2.x}, {n.v2.y})\n");
