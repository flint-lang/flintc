use Core.assert
use Core.print
use Core.system

#test_output_always
test "Always printing output":
	print("Wahooo\n");
	print("Wololololo\n");

test "You should see nothing":
	print("Wahooo\n");
	print("Wololololo\n");

test "You should see this output":
	print("Wahooo\n");
	print("Wololololo\n");
	assert(false);

def main():
	start_capture();
	for (i, _) in 0..10:
		print($"line {i}\n");
	str capture = end_capture();
	print($"capture = \"{capture}\"\n");
	

	start_capture();
	for (i, _) in 0..10:
		print($"line {i}\n");
	str[] lines = end_capture_lines();
	print($"line_count: {lines.len}\n");
	for (i, line) in lines:
		print($"lines[{i}] | {line}\n");
