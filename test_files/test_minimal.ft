use Core.print

data Container:
	opaque value;
	u64 len;
	Container(value, len);

extern def create() -> Container;
extern def destroy(mut Container* c);

def main():
	c := Container(null, 0);
	print($"c.value = {c.value}\n");
	print($"c.len = {c.len}\n");

	con := create();
	print($"c.value = {con.value}\n");
	print($"c.len = {con.len}\n");
	destroy(&con);
