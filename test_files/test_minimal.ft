use Core.print

def set_value(mut i32? opt, i32 val):
    opt = val;

def equals(i32? lhs, i32? rhs) -> bool:
	return lhs == rhs;

def has_value(i32? opt) -> bool:
	return opt != none;

def main():
	i32? maybe = 69;
	print($"has_value = {has_value(maybe)}\n");

	i32? other = 69;
	print($"equals = {equals(maybe, other)}\n");

	set_value(maybe, 17);
	print($"maybe = {maybe!}\n");
