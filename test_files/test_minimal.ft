use Core.print

data SomeData:
    i32 x;
    f32 y;
    str m;
    SomeData(x, y, m);

def main():
    data<i32, f32, str> tuple = (3, 2.2, "hello!");

    i32 first = tuple.$0;
    f32 second = tuple.$1;
    str third = tuple.$2;

    print($"first = {first}\n");
    print($"second = {second}\n");
    print($"third = {third}\n");

    SomeData sd = SomeData(3, 2.2, "hello!");
    print($"sd.(x, y, m) = ({sd.x}, {sd.y}, \"{sd.m}\")\n");