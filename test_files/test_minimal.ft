use Core.print

type Color u8x4

data MyData:
	i32 x = 10;
	Color c = (10, 20, 30, 40);
	Color c2 = Globals.WHITE;
	MyData(x, c, c2);

const data Globals:
	Color WHITE = (245, 245, 245, 255);

def main():
	Color c = Globals.WHITE;
	print($"c = {c}\n");

	d := MyData(_, _, _);
	print($"d.x = {d.x}\n");
	print($"d.c = {d.c}\n");
	print($"d.c2 = {d.c2}\n");

	d2 := MyData(_);
	print($"d2.x = {d2.x}\n");
	print($"d2.c = {d2.c}\n");
	print($"d2.c2 = {d2.c2}\n");
