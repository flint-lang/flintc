use Core.print

data Vec2:
	i32 x;
	i32 y;
	Vec2(x, y);

def main():
	i32? maybe = 10;
	i32 val = maybe!;
	print($"val = {val}\n");

	Vec2 v2 = Vec2(10, 20);
	Vec2? vec = v2;
	i32 y = vec!.y;
	print($"y = {y}\n");
	i32 x = vec!.x;
	print($"x = {x}\n");
	(x, y) = vec!.(y, x);
	print($"(x, y) = ({x}, {y})\n");

	i32[]? arr = i32[10](0);
	i32 n3 = arr![3];
	print($"n3 = {n3}\n");

