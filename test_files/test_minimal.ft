use Core.print

variant Var:
	i32, f32, bool;

def main():
	Var my_var = 5;
	i32 constant = 10;

	i32 res = switch my_var:
		i32(i) -> i + 3;
		f32(f) -> constant + 2;
		bool(b) -> constant + 3;
	print($"res = {res}\n");

	my_var = 3.4;
	res = switch my_var:
		i32(i) -> i + 3;
		f32(f) -> constant + 2;
		bool(b) -> constant + 3;
	print($"res = {res}\n");

	my_var = false;
	res = switch my_var:
		i32(i) -> i + 3;
		f32(f) -> constant + 2;
		bool(b) -> constant + 3;
	print($"res = {res}\n");
