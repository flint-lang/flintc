use Core.print

data MyData:
	i32 x;
	i32 y;
	i32 z;
	MyData(x, y, z);

func MyFunc requires(MyData d):
	def print():
		print($"d = ({d.x}, {d.y}, {d.z})\n");
	
	def get_x() -> i32:
		return d.x;

entity MyEntity:
	data:
		MyData;
	func:
		MyFunc;
	MyEntity(MyData);

def main():
	d := MyData(10, 20, 30);
	MyFunc.print(d);
	x := MyFunc.get_x(d);
	print($"d.x = {x}\n");

