use Core.print

use "small_multi_1.ft"

func MyFunc requires(BaseData d):
	def print():
		print($"b = {d.speed}\n");
	
	def get_x() -> i32:
		return 0;

entity MyEntity extends(BaseEntity e):
	data;
	func: MyFunc;
	MyEntity(e);

def somefn(MyEntity e):
	e.print();

def main():
	d := BaseData(10);
	MyFunc.print(d);
	x := MyFunc.get_x(d);
	print($"d.x = {x}\n");

	be := BaseEntity(BaseData(10));
	be.base();

	me := MyEntity(BaseData(10));
	me.base();
	me.print();
	x = me.get_x();
	print($"me.get_x() = {x}\n");
	somefn(me);
