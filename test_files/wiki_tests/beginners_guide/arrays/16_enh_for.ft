use Core.print

def main():
    i32[] arr = i32[5](0);
    // First, fill the array with meaningful values
    for u32 i = 0; i < arr.length; i++:
        arr[i] = i32(i * 2);

    // Iterate through the array element by element
    for (idx, elem) in arr:
        print($"Index {idx}, Value {elem}\n");
