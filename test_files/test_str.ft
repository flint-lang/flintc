def add(i32 x, i32 y) -> i32:
    return x + y;

def add_str(str a, str b) -> str:
    return a + b;

def test_str():
    for i32 i = 0; i < 5; i++:
        for i32 j = 0; j < 5; j++:
            i32 res = add(i, j);
            if res == 5:
                print(add_str(res, " == 5\n"));
            else:
                print($"{res} != 5\n");